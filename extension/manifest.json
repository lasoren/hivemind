{
  "name": "HiveMind Live",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension interfaces with HiveMind and provides sentiment for the relevant information on a page",
  "homepage_url": "http://swarmtheinter.net",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "js/jquery/jquery.js",
        "js/jquery/jquery-replacetext.js",
        "src/inject/inject.js"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "js/jquery/jquery.js",
        "js/jquery/jquery-replacetext.js",
        "src/inject/inject.js"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ],
    "permissions": [
      "tabs", "https://www.google.com/*", "http://www.google.com/*", "http://54.84.224.246/*", "http://*/*", "https://*/*"
  ]
}
